.container {
	width: 600rpx;
    height: 600rpx;
    margin: 0 auto;
    margin-top:250rpx;
	display: flex;
	display:-webkit-flex;
	flex-wrap:wrap;
	justify-content:space-around;
	align-items: center;
}

.grid-container {
	box-sizing: border-box;
}

.grid {
	width: 100%;
	height: 100%;
	background-image:url(https://sg.eldesign.cn/square.png); 
	background-size:100% 100%;
	display: flex;
	justify-content:center;
	align-items:center;
	border-radius: 5px;
	font-size: 8vw;
	color: rgb(0, 78, 102);

}