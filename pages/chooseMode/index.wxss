

.level-container{
    width: 100%;
    height:600rpx;
    margin-top: 200rpx;
    display: flex;
    justify-content:center;
	align-items:center;
    flex-flow: row wrap;
}

.level-container .level-item{
    width:160rpx;
    height:160rpx;
    margin-left:30rpx;
    background:rgba(157, 238, 238, 0.7);
    display: flex;
	justify-content:center;
    align-items:center;
    color: aliceblue;

}

.mask{
    flex:1;
    width:160rpx;
    height:160rpx;
    margin-left:30rpx;
    margin-top:-160rpx;
    text-align: center;
    background:rgba(100, 95, 95, 0.3);
}
.lock-icon{
    width:50rpx;
    height:50rpx;
    position: relative;
    top: 50rpx;
}