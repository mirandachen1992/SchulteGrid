@font-face{
	font-family: 'hyquheiw';
	src: url("https://sg.eldesign.cn/SchulteGrid/fonts/hyquheiw.ttf") format('truetype'),
	url("https://sg.eldesign.cn/SchulteGrid/fonts/hyquheiw.woff") format('woff'),
	url("https://sg.eldesign.cn/SchulteGrid/fonts/hyquheiw.eot") ,
	url("https://sg.eldesign.cn/SchulteGrid/fonts/hyquheiw.svg") ,format('svg')

}
.app {
	padding: 20rpx 60rpx;
}
.area {
	width: 600rpx;
	height: 600rpx;
	margin: 60px auto;
	border-radius: 10px;
	position: relative;
	
}
.time-recorder{
	text-align: center;
}
.container {
	width: 100%;
	height: 100%;
	display: flex;
	display:-webkit-flex;
	flex-wrap:wrap;
	justify-content:space-around;
	align-items: center;
	/* background: #c5c5c5; */
}

.grid-container {
	/* background: #c5c5c5; */
	box-sizing: border-box;
}

.grid {
	width: 100%;
	height: 100%;
	background-image:url(https://sg.eldesign.cn/square.png); 
	background-size:100% 100%;
	display: flex;
	justify-content:center;
	align-items:center;
	border-radius: 5px;
	font-size: 8vw;
	color: rgb(0, 78, 102);
	transition: all 0.2s;

}

.grid-disable {
	border: none;
	background: url("data:image/png;base64,") no-repeat center;
	background-size:90rpx 90rpx;
	/* transform: rotateY(180deg); */
}

.test:active {
	background: #f19b9b;
	border-color: #f19b9b; 
}

.grid-disable text {
	display: none;
}

.shadow {
	position: absolute;
	width: 100%;
	height: 100%;
	background: #ffffff;
	opacity: 0.1;
	/*border: 10rpx solid #f3d665;*/
	border-radius: 10px;

}
.buttons-group {
	position: absolute;
	width: 90%;
	bottom: 0;
	padding: 0 5% 0 5%;
}
.buttons {
	display: flex;
	justify-content:space-around;
	margin-bottom: 40rpx;
}

.buttons .normal-tab{
	width:114rpx;
	height:114rpx;
	background:none;
	color:#ffffff;
	border:5rpx solid #ffffff;
	border-radius: 15rpx;
	box-shadow: 2px 2px 1px rgba(0,0,0,0.2);
	font-size: 28rpx;
	font-family: "Lantinghei SC";
	font-weight: 700;
	line-height: 116rpx;
	text-align: center;
	transform:rotate(-20deg);
	-ms-transform:rotate(-20deg); 	/* IE 9 */
	-moz-transform:rotate(-20deg); 	/* Firefox */
	-webkit-transform:rotate(-20deg); /* Safari 和 Chrome */
	-o-transform:rotate(-20deg); 	/* Opera */
}
.buttons .active-tab {
	/*color: #efcb48;*/
	/* background: #fcad26; */
	width:78rpx;
	height:78rpx;
	background: #ffffff;
	color:rgb(25, 150, 174);
	border:none;
	border-radius: 20rpx;
	font-size: 28rpx;
	font-family: "Lantinghei SC";
	font-weight: 700;
	line-height: 70rpx;
	text-align: center;
	margin-top:18rpx;
	margin-left:20rpx;
}
.buttons-bottom {
	/*display: flex;*/
	/*justify-content:space-around;*/
}
.buttons-bottom button {
	width: 233rpx;
	margin-bottom: 30rpx;
	height: 74rpx;
	line-height: 70rpx;
	border-radius: 40rpx;
	font-size: 24rpx;
	color: #ffffff;

}

.buttons-bottom .premary-button {
	background:url(https://sg.eldesign.cn/SchulteGrid/img/button_goon.png) no-repeat center;
}

.buttons-bottom .premary-button:active {
	/* background: #f39b04; */
}


.buttons-bottom .default-button {
	color: #ffffff;
	background: url(https://sg.eldesign.cn/SchulteGrid/img/button_share.png) no-repeat center;
	/* color: #5cbcf6; */
	/* border: 1px solid #5cbcf6; */
	/* background: transparent; */

}
.buttons-bottom .active-button {
	color: #ffffff;
	background: url(https://sg.eldesign.cn/SchulteGrid/img/button_share.png) no-repeat center;
	/* color: #fcad26; */
	/* border: 1px solid #fcad26; */
	/* background: transparent; */

}

.text {
	margin-bottom:30rpx;
	text-align:center;
	color:#ffffff;
}
.text .status {
	font-size:60rpx;
}
.text .score {
	color:rgb(244, 249, 61);
	font-size: 130rpx;
	font-family: 'hyquheiw';

}
.text .score text {
	font-family: 'hyquheiw';
}
.card view {
	font-size: 20rpx;
}
.card {
	background: #a09b92;
	color: #ffffff;
	display: inline-block;
	padding: 10rpx 20rpx;
	float: right;
	border-radius: 10rpx;
	margin: 0 0 20rpx 20rpx;
	font-size: 30rpx;
	min-width: 80rpx;
	text-align: center;
}

.avatar {
	border-radius: 50%;
	overflow: hidden;
	display: block;
	margin: auto;
}



.avatar-container-small {
	margin: 50rpx 0 20rpx;
	text-align: center;
}
.avatar-container-small .avatar {
	width: 100rpx;
	height: 100rpx;
}

.avatar-container-small .avatar-name {
	color: #ffffff;
	font-size: 30rpx;
}

.avatar {
	transition: width 0.5s ease-out, height 0.5s ease-out;
	-moz-transition: width 0.5s ease-out, height 0.5s ease-out;	/* Firefox */
	-webkit-transition: width 0.5s ease-out, height 0.5s ease-out;	/* Safari 和 Chrome */
	-o-transition: width 0.5s ease-out, height 0.5s ease-out;
	/*transition: height 5s;
	-moz-transition: height 5s;	
	-webkit-transition: height 5s;	
	-o-transition: height 5s;*/
}

.avatar-name {
	transition: font-size 0.5s ease-out;
	-moz-transition: font-size 0.5s ease-out;	/* Firefox */
	-webkit-transition: font-size 0.5s ease-out;	/* Safari 和 Chrome */
	-o-transition: font-size 0.5s ease-out;
}

.authorize-button {
	width: 80%;
}